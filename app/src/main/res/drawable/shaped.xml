<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval" >
<corners
    android:radius="2dp"
    android:topLeftRadius="4dp"
    android:topRightRadius="4dp"
    android:bottomLeftRadius="4dp"
    android:bottomRightRadius="4dp" />
<gradient
    android:endColor="#012348"
    android:startColor="#000FFF"
    android:type="linear"
    android:useLevel="true"/>
<padding
    android:left="4dp"
    android:top="4dp"
    android:right="4dp"
    android:bottom="4dp" />
<size
    android:width="8dp"
    android:height="3dp" />
<solid
    android:color="#fff879" />
<stroke
    android:width="1dp"
    android:color="#567909"
    android:dashWidth="1dp"
    android:dashGap="1dp" />
</shape>